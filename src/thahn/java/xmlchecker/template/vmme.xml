<?xml version='1.0' encoding='UTF-8'?>
<vnfd xmlns="urn:ietf:params:xml:ns:yang:nfvo" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="urn:ietf:params:xml:ns:yang:nfvo vnfd-1.xsd">
	<vnfd_id></vnfd_id>
	<name></name>
	<provider></provider>
	<description></description>
	<descriptor_version></descriptor_version>
	<descriptor_tag __ignored="true"></descriptor_tag>
	<version>test1|test2|test3</version>
	<vnfcs __description="vnfcs des"
		   testAttr1="test1|test2|test3" 
		   testAttr1__description="test123 des"
		   testAttr2="" >
		<vnfc>
			<vnfc_id __required="true"></vnfc_id>
			<name></name>
	        <vm_spec>
		        <pkg_uri></pkg_uri>
		    </vm_spec>
			<high_availability></high_availability>
			<cpu>
				<num_vpu></num_vpu>
				<vcpu_topology></vcpu_topology>
				<properties>
					<property>
						<id></id>
						<description></description>
						<value></value>
					</property>
				</properties>
			</cpu>
			<memory>
				<total_memory_gb></total_memory_gb>
				<properties>
					<property>
						<id></id>
						<description></description>
						<value></value>
					</property>
				</properties>
			</memory>
			<storages>
				<storage>
					<storage_id></storage_id>
					<name></name>
					<size_gb></size_gb>
					<type></type>
				</storage>
			</storages>
			<network_interfaces>
				<network_interface>
					<name></name>
					<description></description>
					<ivl_id_ref></ivl_id_ref>
					<properties>
						<property>
							<id></id>
							<description></description>
							<value></value>
						</property>
					</properties>
				</network_interface>
			</network_interfaces>
			<platform_pcie_parameters>
				<parameter>
					<id></id>
					<description></description>
					<value></value>
				</parameter>
			</platform_pcie_parameters>
			<hypervisor_parameters>
				<parameter>
					<id></id>
					<description></description>
					<value></value>
				</parameter>
			</hypervisor_parameters>
			<constraints>
				<constraint>
					<constraint_id></constraint_id>
					<description></description>
					<value></value>
				</constraint>
			</constraints>
			<personality>
				<path></path>
				<contents>@(([a-zA-Z0-9,_]*=[a-zA-Z0-9,_]*){0,1}&#10;)+|([0-9]+:'[0-9]+',{0,1})+</contents>
			</personality>
			<lifecycle_events>
				<lifecycle_event>
					<id></id>
					<name></name>
					<script />
					<description />
				</lifecycle_event>
			</lifecycle_events>
		</vnfc>
	</vnfcs>
	<connection_points>
		<connection_point>
			<description></description>
			<type></type>
			<ivl_id_ref></ivl_id_ref>
			<cp_id></cp_id>
			<name></name>
		</connection_point>
	</connection_points>
	<internal_virtual_links>
		<internal_virtual_link>
			<connectivity_type></connectivity_type>
			<qos>
				<latency></latency>
			</qos>
			<name></name>
			<enable_dhcp></enable_dhcp>
			<ivl_id></ivl_id>
			<properties>
				<property>
					<id></id>
					<description></description>
					<value></value>
				</property>
			</properties>
		</internal_virtual_link>
	</internal_virtual_links>
	<monitoring_params>
		<monitoring_param>
			<param_id></param_id>
			<description></description>
		</monitoring_param>
	</monitoring_params>
	<flavours>
		<flavour>
			<flavour_id></flavour_id>
			<name></name>
			<description></description>
			<assurance_params>
				<param_id></param_id>
				<value></value>
			</assurance_params>
			<vdus>
				<vdu>
					<vdu_id></vdu_id>
					<vnfc_id></vnfc_id>
					<number_of_instances></number_of_instances>
					<scale_in_out>
						<min></min>
						<max></max>
					</scale_in_out>
				</vdu>
			</vdus>
		</flavour>
	</flavours>
	<vdu_dependencies>
		<vdu_dependency>
			<sequence_id></sequence_id>
			<vdu_id_ref __dependency="vnfd.flavours.flavour.vdus.vdu.vdu_id"></vdu_id_ref>
			<depends_on>
				<vdu_id_ref __dependency="vnfd.flavours.flavour.vdus.vdu.vdu_id"></vdu_id_ref>
			</depends_on>
			<create_after>
				<vdu_id_ref __dependency="vnfd.flavours.flavour.vdus.vdu.vdu_id"></vdu_id_ref>
			</create_after>
		</vdu_dependency>
	</vdu_dependencies>
	<autoscaling_policies>
		<autoscaling_policy>
			<scaling_policy_id></scaling_policy_id>
			<name></name>
			<severity></severity>
			<source_flavour_id></source_flavour_id>
			<target_flavour_id></target_flavour_id>
			<target_vdu_id></target_vdu_id>
			<expression>
				<monitoring_param_id_ref __dependency="vnfd.monitoring_params.monitoring_param.param_id">
				</monitoring_param_id_ref>
				<operator></operator>
				<threshold></threshold>
				<holdtime_sec></holdtime_sec>
				<cooldown_sec></cooldown_sec>
			</expression>
		</autoscaling_policy>
	</autoscaling_policies>
	<lifecycle_events>
		<lifecycle_event>
			<id></id>
			<name></name>
			<script />
			<description />
		</lifecycle_event>
	</lifecycle_events>
</vnfd>
